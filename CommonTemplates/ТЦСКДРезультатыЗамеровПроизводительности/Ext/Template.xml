	<totalField>
		<dataPath>APDEX</dataPath>
		<expression>ВЫБОР
		КОГДА СУММА(КоличествоЗамеров) &gt; 0
			ТОГДА (СУММА(Т) + СУММА(Т4_Пополам)) / СУММА(КоличествоЗамеров)
		ИНАЧЕ 0
	КОНЕЦ</expression>
	</totalField>
	<totalField>
		<dataPath>СтандартноеОтклонение</dataPath>
		<expression>SQRT(Минимум(Дисперсия))</expression>
		<group>КлючеваяОперация</group>
	</totalField>
	<totalField>
		<dataPath>ЦелевоеВремя</dataPath>
		<expression>Минимум(ЦелевоеВремя)</expression>
		<group>КлючеваяОперация</group>
	</totalField>
	<totalField>
		<dataPath>Продолжительность.ВремяВыполненияСреднее</dataPath>
		<expression>ВЫБОР
		КОГДА СУММА(КоличествоЗамеров) &gt; 0
			ТОГДА Сумма(ВремяВыполненияСуммарное) / СУММА(КоличествоЗамеров)
		ИНАЧЕ 0
	КОНЕЦ</expression>
	</totalField>
